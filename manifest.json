{
	"name": "I-kiu Typo3 Page ID",
	"version": "1.0",
	"description": "Show the Page ID if present",
	"permissions": [ "tabs", "contextMenus", "http://*/*", "https://*/*","activeTab","declarativeContent","storage"],
	"options_page": "options.html",
	"background": {
	  "scripts": ["background.js"],
	  "persistent": true
	},
	"content_scripts": [ {
      "js": [ "content_script.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
	 "browser_action": {
          "default_title": "I-kiu Typo3 Page ID",
					"default_popup": "popup.html",
		      "default_icon": {
		        "16": "images/folder_16.png",
		        "24": "images/folder_24.png",
		        "32": "images/folder_32.png",
		        "128": "images/folder_128.png"
		      }
  },
  "icons": {
			"16": "images/folder_16.png",
			"24": "images/folder_24.png",
			"32": "images/folder_32.png",
			"128": "images/folder_128.png"
    },
    "manifest_version": 2
}
